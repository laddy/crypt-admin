<% extend 'layout.ect' %>


<div class="container">

<h1>{{UserID}}のリスト</h1>

<table class="table table-striped">
<tr>
    <th>#</th>
    <th>Service</th>
    <th>URL</th>
    <th>ID</th>
    <th>Pass</th>
</tr>
<% for link in @service : %>
<tr>
    <td><%- link._id %></td>
    <td><%- link.service %></td>
    <td><%- link.url %></td>
    <td><%- link.id %></td>
    <td><%- link.pass %></td>
</tr>
<% end %>
</table>

</div>
